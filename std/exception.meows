    InvalidBox:             __exc("InvalidBox"),
    DivisionByZero:         __exc("DivisionByZero"),
    Argument:               __exc("Argument"),
    InvalidToken:           __exc("InvalidToken"),
    InvalidFunction:        __exc("InvalidFunction"),
    InvalidImport:          __exc("InvalidImport"),
    InvalidInnerFunction:   __exc("InvalidInnerFunction"),
    InvalidValue:           __exc("InvalidValue"),
    CatOnComputer:          __exc("CatOnComputer"),
    KeywordException:       __exc("KeywordException"),
    File:                   __exc("File"),
    FunctionDefinition:     __exc("FunctionDefinition"),
    Hashing:                __exc("Hashing"),
    Unexpected:             __exc("Unexpected")
]
